<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="CITS.ResultsPage">
	<ContentPage.Content>
            <StackLayout Orientation="Vertical" Margin= "0,20,0,0">
        <Label VerticalOptions="Center" HorizontalOptions="Center">Results Page</Label>
            <Label x:Name="ResultsTotalLabel" VerticalOptions="Center" HorizontalOptions="Center" Margin="10,20,5,5"></Label>
            <Label x:Name="ResultsGeneratedProblemsLabel" VerticalOptions="Center" HorizontalOptions="Center" Margin="10,20,5,5"></Label>
            <Label x:Name="RecommendationLabel" VerticalOptions="Center" HorizontalOptions="Center" Margin="10,20,5,5"></Label> 
            <Label x:Name="ConfidenceAverageLabel" VerticalOptions="Center" HorizontalOptions="Center" Margin="10,20,5,5"></Label> 
            <Button Text="Dismiss Results Screen" Clicked="OnDismissButtonClicked"/>
      </StackLayout>
	</ContentPage.Content>
</ContentPage>
